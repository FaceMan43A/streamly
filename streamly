  <script>
    function showCategory(categoryId, event) {
      // Skryť všetky kategórie
      const categories = document.querySelectorAll('.category');
      categories.forEach(cat => cat.style.display = 'none');

      // Zobraziť vybranú kategóriu
      const selected = document.getElementById(categoryId);
      if (selected) selected.style.display = 'block';

      // Odstrániť aktívny stav zo všetkých tlačidiel
      const buttons = document.querySelectorAll('nav button');
      buttons.forEach(btn => btn.classList.remove('active'));

      // Pridať aktívny stav kliknutému tlačidlu
      if (event && event.currentTarget) {
        event.currentTarget.classList.add('active');
      }
    }
  </script>
